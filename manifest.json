{
    "author": "Siberis",
    "background": {
        "page": "background.html",
        "persistent": true
    },
    "content_scripts": [
        {
            "js": [
                "js/keypress.js"
            ],
            "matches": [
                "*://*.feedly.com/*",
                "*://feedly.com/*"
            ]
        }
    ],
    "description": "Open Feedly News in Background Tab",
    "icons": {
        "128": "images/icon_128.png"
    },
    "manifest_version": 2,
    "name": "FeedlyBackTab",
    "options_page": "options.html",
    "permissions": [
        "http://www.feedly.com/*",
        "https://www.feedly.com/*",
        "storage"
    ],
    "version": "0.1"
}